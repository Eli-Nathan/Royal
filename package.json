{
  "name": "royal",
  "version": "1.0.0",
  "description": "Royal Dunkeld hotel website",
  "main": "gulpfile.js",
  "scripts": {
    "setup": "npm install -g gulp-cli && npm install && gem install bundler && bundle install",
    "dev": "gulp server",
    "force": "gulp javascripts --force && gulp stylesheets --force && jekyll build",
    "upgrade": "npm install -g npm-check-updates && npm-check-updates -u && npm install && gem install bundler && bundle update",
    "crunch": "gulp optimize_images"
  },
  "author": "Eli Nathan",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "*",
    "browserify": "*",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "es6-promise": "^4.1.1",
    "event-stream": "^3.3.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "*",
    "gulp-buffer": "*",
    "gulp-changed": "*",
    "gulp-debug": "*",
    "gulp-front-matter": "*",
    "gulp-if": "*",
    "gulp-imagemin": "*",
    "gulp-newer": "*",
    "gulp-no-media-queries": "*",
    "gulp-rename": "*",
    "gulp-sass": "*",
    "gulp-tap": "*",
    "gulp-uglify": "*",
    "liquid-node": "*",
    "gulp4-run-sequence": "*",
    "vinyl-buffer": "*",
    "yargs": "*"
  },
  "keywords": [
    "jekyll",
    "gulp",
    "sass",
    "browsersync"
  ],
  "dependencies": {
    "jquery": "^3.3.1",
    "bootstrap": "^4.0.0",
    "gulp-mamp": "0.0.5",
    "moment": "^2.22.1",
    "popper.js": "^1.14.1",
    "server": "^1.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Eli-Nathan/royal.git"
  }
}
